{
  "name": "secst",
  "version": "0.0.1",
  "description": "SECST is a semantic, extensible, computational, styleable tagged markup language. You can use it to joyfully create compelling, interactive documents backed by HTML.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "(npm run build-browser || npm run build-server) &&  npm run build-server",
    "build-browser": "webpack --config=webpack.browser.config.cjs",
    "build-server": "cp ./src/index.js ./index.js",
    "transform": "node transform",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anywhichway/secst.git"
  },
  "keywords": [
    "Markdown",
    "HTML",
    "markup",
    "semantic",
    "computational",
    "styleable"
  ],
  "author": "Simon Y. Blackwell",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/anywhichway/secst/issues"
  },
  "homepage": "https://github.com/anywhichway/secst#readme",
  "dependencies": {
    "@types/node": "^18.11.9",
    "cross-fetch": "^3.1.5",
    "happy-dom": "^7.7.0",
    "jsdom": "^20.0.3",
    "pegjs": "^0.10.0",
    "string-replace-async": "^3.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^5.0.1",
    "rollup": "^3.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-node-externals": "^3.0.0"
  }
}
